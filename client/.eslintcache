n\n  const submit = async (e) => {\n    e.preventDefault();\n\n    try {\n      const newUser = { email, password, passwordCheck, labName,location, phone,officialWebSite };\n      await Axios.post(\"/users/register\", newUser);\n      const loginRes = await Axios.post(\"/users/login\", {\n        email,\n        password,\n      });\n      setUserData({\n        token: loginRes.data.token,\n        user: loginRes.data.user,\n      });\n      localStorage.setItem(\"auth-token\", loginRes.data.token);\n      history.push(\"/login\");\n    } catch (err) {\n      err.response.data.msg && setError(err.response.data.msg);\n    }\n  };\n\n  return (\n    <div>\n            <br />\n            <div className = \"container text-center\"></div>\n    <div className=\"page\">\n    <h3 className = \"mb-3\">Register</h3>\n      {error && (\n        <ErrorNotice message={error} clearError={() => setError(undefined)} />\n      )}\n      \n      <form className=\"text-center border border-light p-9\" onSubmit={submit}>\n        <label >Email</label>\n        <input\n        required='true' type='text'className=\"form-control col\"\n          id=\"register-email\"\n          type=\"email\"\n          onChange={(e) => setEmail(e.target.value)}\n        />\n\n        <label >Password</label>\n        <input\n        required='true'  type=\"password\" name=\"password\" className=\"form-control col\"\n          id=\"register-password\"\n          type=\"password\"\n          onChange={(e) => setPassword(e.target.value)}\n        />\n        <br></br>\n        <input\n        required='true'  type=\"password\" name=\"password\" className=\"form-control col\"\n          type=\"password\"\n          placeholder=\"Verify password\"\n          onChange={(e) => setPasswordCheck(e.target.value)}\n        />\n\n        <label>Lab Name</label>\n        <input\n        required='true'  className=\"form-control col\"\n          id=\"register-lab-name\"\n          type=\"text\"\n          onChange={(e) => setLabName(e.target.value)}\n        />\n\n<label>Location</label>\n        <input\n        required='true'  className=\"form-control col\"\n          id=\"register-location\"\n          type=\"text\"\n          onChange={(e) => setLocation(e.target.value)}\n        />\n\n<label htmlFor=\"register-phone\">Phone</label>\n        <input\n        required='true'  className=\"form-control col\"\n          id=\"register-phone\"\n          type=\"text\"\n          onChange={(e) => setPhone(e.target.value)}\n        />\n\n<label >Official officialWebSite</label>\n        <input\n        required='true'  className=\"form-control col\"\n          id=\"official-web-site\"\n          type=\"text\"\n          onChange={(e) => setOfficialWebSite(e.target.value)}\n        />\n        \n       {/* < Button class=\"btn btn-secondary\" type='submit' value='Creat Account' className=\"btn btn-deep-orange darken-4\"></Button> */}\n        <input type=\"submit\" value=\"Register\" />\n\n        <br></br>\n               <b>If you already have an account<a href='/login'> Log In </a></b>\n      </form>\n    </div>\n    </div>\n  );\n}\n","/Users/rbk04/Desktop/labs/client/src/context/userContext.js",[],"/Users/rbk04/Desktop/labs/client/src/components/auth/AuthOptions.js",[],"/Users/rbk04/Desktop/labs/client/src/components/firebase.js",[],{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","severity":1,"message":"104","line":44,"column":11,"nodeType":"105","endLine":44,"endColumn":23},{"ruleId":"103","severity":1,"message":"104","line":52,"column":11,"nodeType":"105","endLine":52,"endColumn":26},{"ruleId":"103","severity":1,"message":"104","line":57,"column":11,"nodeType":"105","endLine":57,"endColumn":23},{"ruleId":"103","severity":1,"message":"104","line":65,"column":11,"nodeType":"105","endLine":65,"endColumn":26},{"ruleId":"103","severity":1,"message":"104","line":71,"column":11,"nodeType":"105","endLine":71,"endColumn":26},"no-native-reassign",